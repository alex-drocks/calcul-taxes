{
  "hosting": {
	"public": "out",
	"ignore": [
	  "firebase.json",
	  "**/.*",
	  "**/node_modules/**"
	],
	"rewrites": [
	  {
		"source": "/about",
		"destination": "/about.html",
		"type": 301
	  }
	],
	"headers": [
	  {
		"source": "**/*.@(jpg|jpeg|gif|png|webp|webm|eot|otf|ttf|ttc|woff|woff2|js|css)",
		"headers": [
		  {
			"key": "Cache-Control",
			"value": "max-age=9999999"
		  }
		]
	  }
	],
	"cleanUrls": true,
	"trailingSlash": false
  }
}
